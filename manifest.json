{
    "name": "Vinilos for the win",
    "description": "",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "js/main.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "images/icon.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "notifications"
    ]
}